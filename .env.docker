# Copy this file to .env and fill in your actual values
# Used by docker-compose.yml

# Vapi API Configuration
VAPI_API_TOKEN=your_vapi_api_token_here

# Assistant IDs
MORNING_ASSISTANT_ID=your_morning_assistant_id_here
EVENING_ASSISTANT_ID=your_evening_assistant_id_here

# Phone Configuration
PHONE_NUMBER_ID=your_vapi_phone_number_id_here
TARGET_PHONE_NUMBER=+1234567890

# Optional: Customize call times (cron format)
# Default: Morning at 8:00 AM (0 8 * * *), Evening at 8:00 PM (0 20 * * *)
# MORNING_CALL_TIME=0 8 * * *
# EVENING_CALL_TIME=0 20 * * *

# Optional: Set timezone (default: America/New_York)
# TZ=Europe/Oslo
